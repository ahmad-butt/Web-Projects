ice }] = useStateValue();\n    return (\n        <div className='subtotal'>\n            <div className='subtotal__info'>\n                <p className='subtotal__info'>Subtotal ({`${basket?.length}`} items): <strong>${totalPrice}</strong></p>\n                <p className='subtotal__info'><input type='checkbox'/> This order contains a gift</p>\n            </div>\n            <button className='subtotal__button'><span className='subtotal__buttonInfo'>Proceed to Checkout</span></button>\n        </div>\n    )\n}\n\nexport default SubTotal","/home/ahmad/University/JavaScript/react-website/src/Login.js",["72","73"],"import React, { useState, useEffect } from 'react'\nimport { Link, useHistory } from 'react-router-dom'\nimport { auth } from './firebase'\nimport './Login.css'\nfunction Login() {\n\n    const [email, setEmail] = useState('');\n    const [password, setPassword] = useState('');\n    // const [user, setUser] = useState(null);\n    const history = useHistory();\n\n    const login = (event) => {\n        event.preventDefault();\n        auth.signInWithEmailAndPassword(email, password)\n            .then(\n                console.log('Creating User'),\n                history.push('/'),\n            )\n            .catch((error) => alert(error.message));\n    }\n    const signup = (event) => {\n        event.preventDefault();\n        auth.createUserWithEmailAndPassword(email, password)\n            .then(\n                console.log('Signed up'),\n                history.push('/'),\n            )\n            .catch(error => alert(error.message));\n    }\n    return (\n        <div className='login'>\n            <Link to='/'>\n                <img className='login__image' src='https://upload.wikimedia.org/wikipedia/commons/thumb/a/a9/Amazon_logo.svg/1000px-Amazon_logo.svg.png' />\n            </Link>\n            <form className='login__form'>\n                <h1 className='form__h1'>Sign-in</h1>\n                <strong className='form__strong'>E-mail</strong>\n                <div>\n                    <input className={'form__input'} value={email} onChange={(event) => setEmail(event.target.value)} type='text' />\n                </div>\n                <strong className='form__strong'>Password</strong>\n                <div>\n                    <input className={'form__input'} value={password} onChange={(event) => setPassword(event.target.value)} type='password' />\n                </div>\n                <button className='login__button1' onClick={login}>Sign-in</button>\n                <p className='login__formCondition'>By continuing, you agree to Amazon's Conditions of Use and Sale. Please see our Privacy Notice, our Cookies Notice and our Interest-Based Ads Notice. </p>\n                <button className='login__button2' onClick={signup}>Create your Amazon Account</button>\n            </form>\n        </div>\n    )\n}\n\nexport default Login\n","/home/ahmad/University/JavaScript/react-website/src/firebase.js",[],{"ruleId":"74","replacedBy":"75"},{"ruleId":"76","replacedBy":"77"},{"ruleId":"78","severity":1,"message":"79","line":4,"column":42,"nodeType":"80","messageId":"81","endLine":4,"endColumn":46},{"ruleId":"82","severity":1,"message":"83","line":32,"column":6,"nodeType":"84","endLine":32,"endColumn":24,"suggestions":"85"},{"ruleId":"78","severity":1,"message":"86","line":4,"column":8,"nodeType":"80","messageId":"81","endLine":4,"endColumn":18},{"ruleId":"78","severity":1,"message":"87","line":6,"column":13,"nodeType":"80","messageId":"81","endLine":6,"endColumn":19},{"ruleId":"88","severity":1,"message":"89","line":31,"column":32,"nodeType":"90","messageId":"91","endLine":31,"endColumn":34},{"ruleId":"78","severity":1,"message":"87","line":6,"column":14,"nodeType":"80","messageId":"81","endLine":6,"endColumn":20},{"ruleId":"92","severity":1,"message":"93","line":20,"column":17,"nodeType":"94","endLine":20,"endColumn":80},{"ruleId":"78","severity":1,"message":"95","line":1,"column":17,"nodeType":"80","